class Solution {
public:
    int findContentChildren(vector<int>& g, vector<int>& s) {
        
      int n = g.size();
      int m = s.size();

      int contChild = 0;
      std::sort(g.begin(), g.end());  
      std::sort(s.begin(), s.end());

      int i=0,j=0;
      while(i<n && j<m)
      {
         if(s[j]>=g[i])
         {
            contChild++;
            i++;
         }
         j++;
      }
        
     return contChild;
    }
};
