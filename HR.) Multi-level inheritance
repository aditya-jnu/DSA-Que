#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    class Equilateral{
    public:
       void triangle(){
         cout<<"I am an equilateral triangle\n";
    }
};

    class Isosceles : public Equilateral{
    public:
        void isosceles(){
            cout<<"I am an isosceles triangle\n";
            cout<<"I am a triangle\n";
        }
};

    
    Isosceles isc;
    isc.triangle();
    isc.isosceles();
    
    return 0; 
}
